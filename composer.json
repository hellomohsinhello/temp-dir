{
    "name": "hellomohsinhello/temp-dir",
    "description": "This package is useful for projects who requires to create temporary directories.",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohsin Ali",
            "email": "hellomohsinhello@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Hellomohsinhello\\TempDir\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hellomohsinhello\\TempDir\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
